/* ----------------------------------------------
	Fibonacci-sorozat Lindemayer-rendszer
				
ábécé: {x}	
helyettesítések: o(a)=b; o(b)=ab
-------------------------------------------------*/

#pragma lindenmayer

start A; nterm B; // 1. kollekció
nterm A2, B2;     // 2. kollekció
term a, b;

void main() 
[
	{
		// átírások
		all A = B2;
		all B = A2 B2;
	
		// visszaírás 1. kollekcióra:
		all A2 = A;
		all B2 = B;
	}
	{ all A = a; all B = b; } 
]